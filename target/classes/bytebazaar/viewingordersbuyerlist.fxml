            <Font size="14.0" />
         </font>
         <padding>
            <Insets left="20.0" />
         </padding>
      </Label>
      <Label fx:id="orderPlacedTime" contentDisplay="RIGHT" prefHeight="34.0" prefWidth="131.0" text="Amount">
         <font>
            <Font size="14.0" />
         </font>
         <padding>
            <Insets left="20.0" />
         </padding>
      </Label>
      <Label fx:id="orderPlacedDate" contentDisplay="RIGHT" prefHeight="34.0" prefWidth="158.0" text="Date">
         <font>
            <Font size="14.0" />
         </font>
         <padding>
            <Insets left="20.0" />
         </padding>
      </Label>
      <Label fx:id="orderPlacedDate1" contentDisplay="RIGHT" layoutX="212.0" layoutY="10.0" prefHeight="34.0" prefWidth="139.0">
         <font>
            <Font size="14.0" />
         </font>
         <padding>
            <Insets left="20.0" />
         </padding>
      </Label>
      <Button fx:id="orderPlacedDetailButton" mnemonicParsing="false" onAction="#TrackDetails" prefHeight="25.0" prefWidth="61.0" text="Track" />
   </children>
