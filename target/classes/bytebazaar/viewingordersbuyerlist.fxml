<HBox fx:id="orderPlacedHBox" alignment="CENTER_LEFT" prefHeight="34.0" prefWidth="566.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="orderPlacedID" contentDisplay="RIGHT" prefHeight="34.0" prefWidth="71.0" text="OrderID">
